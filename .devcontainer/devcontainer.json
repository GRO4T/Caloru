{
	"name": "Caloru",
	"dockerComposeFile": "docker-compose.yml",
	"service": "caloru",
	"workspaceFolder": "/app",
	"forwardPorts": [
		8000
	],
	"portsAttributes": {
		"8000": {
			"label": "Caloru"
		}
	},
	"postCreateCommand": ". $VIRTUAL_ENV/bin/activate && ./scripts/setup.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"streetsidesoftware.code-spell-checker",
				"charliermarsh.ruff"
			]
		}
	}
}
