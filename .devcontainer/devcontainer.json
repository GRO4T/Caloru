{
	"name": "Caloru",
	"dockerComposeFile": "docker-compose.yml",
	"service": "caloru",
	"workspaceFolder": "/app",
	"forwardPorts": [
	  8000
	],
	"portsAttributes": {
	  "8000": {
		  "label": "Caloru"
	  }
	},
	"postCreateCommand": "cd ./caloru && python manage.py migrate",
	"customizations": {
	  "vscode": {
		"extensions": [
          "ms-python.black-formatter",
          "ms-python.pylint",
          "ms-python.python",
          "ms-python.vscode-pylance",
          "streetsidesoftware.code-spell-checker"
		]
	  }
	}
  }